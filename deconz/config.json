{
  "name": "deCONZ",
  "version": "2.05.21",
  "slug": "deconz",
  "description": "deCONZ REST plugin provides a REST API to access ZigBee",
  "startup": "services",
  "boot": "auto",
  "hassio_api": false,
  "homeassistant_api": false,
  "host_network": true,
  "webui": "http://[HOST]:[PORT:8234]/pwa/login.html#host/[HOST]:8234",
  "auto_uart":true,
  "map": [
    "config:rw"
  ],
  "options": {
    "httpport": 8080,
    "wsport": 8088,
    "conbee": true
  },
  "schema": {
    "httpport": "port",
    "wsport": "port",
    "conbee": "bool"
  },
  "ports": {
    "8234/tcp": 8080,
    "8235/tcp": 8088
  }
}
